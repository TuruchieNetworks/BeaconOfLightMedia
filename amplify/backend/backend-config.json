{
  "api": {
    "beaconoflifemedia": {
      "dependsOn": [],
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [],
          "defaultAuthentication": {
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7
            },
            "authenticationType": "API_KEY"
          }
        }
      },
      "providerPlugin": "awscloudformation",
      "service": "AppSync"
    }
  },
  "auth": {
    "BeaconOfLifeMedia": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OPTIONAL",
        "mfaTypes": [
          "SMS",
          "TOTP"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_UPPERCASE"
          ],
          "passwordPolicyMinLength": "6"
        },
        "signupAttributes": [
          "EMAIL",
          "FAMILY_NAME",
          "GIVEN_NAME"
        ],
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL",
          "PHONE_NUMBER"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "storage": {
    "BeaconOfLifeMediaStorage": {
      "dependsOn": [],
      "providerPlugin": "awscloudformation",
      "service": "S3"
    }
  }
}